<% reviews.forEach(function(review) { %>
    <h5 class="album-reviewed col-md-8"><a href="/albums/<%=review.album %>"><%= review.album_reviewed %></a></h5>
    <h5 class="review-date"><%= review.review_date.toDateString().substring(0, 15) %></h5><span><i class="fa fa-trash"></i></span></h5>
    <h5 class="review-content"><%= review.content %></h5>
    <h5 class="space-before-author col-md-7"></h5><h5 class="review-author">by <a href="/users/<%= review.author %>"><%= review.author_name %></a></h5>
  <br>
  <br>
<% }) %>
